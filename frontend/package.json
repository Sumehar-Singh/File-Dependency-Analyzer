{
  "name": "file-dependency-analyzer",
  "version": "1.0.0",
  "private": true,
  "description": "A tool to analyze file dependencies",
  "author": "Sumehar Singh Grewal",
  "copyright": "Copyright Â© 2025 Sumehar Singh Grewal",
  "homepage": "./",
  "main": "main.js",
  "dependencies": {
    "cytoscape": "^3.33.1",
    "cytoscape-dagre": "^2.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.5.1",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "react-scripts": "5.0.1",
    "wait-on": "^8.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "package": "npm run build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "build": {
    "appId": "com.grewal.filedependencyanalyzer",
    "productName": "File Dependency Analyzer",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "package.json",
      {
        "from": "../backend",
        "to": "backend"
      }
    ],
    "extraResources": [
      {
        "from": "../backend/server.js",
        "to": "backend/server.js"
      },
      {
        "from": "../backend/package.json",
        "to": "backend/package.json"
      },
      {
        "from": "../backend/node_modules",
        "to": "backend/node_modules"
      }
    ],
    "asarUnpack": [
      "backend/**/*"
    ],
    "extraMetadata": {
      "main": "main.js"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    }
  }
}
